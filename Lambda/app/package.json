{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=development eslint src/ && npm run server",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/server.js",
    "clean": "rimraf dist",
    "build": "webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.config.js",
    "build:prod": "webpack --mode production --config webpack.server.config.js",
    "prep": "npm run clean && npm run build:prod",
    "watch": "nodemon",
    "prod": "NODE_ENV=production npm run clean && npm run build && npm run server:prod"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^3.1.2",
    "aws-sdk": "^2.548.0",
    "aws-serverless-express": "^3.3.6",
    "cognito-express": "^2.0.16",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongodb-client-encryption": "^0.3.1",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "acorn": "^7.1.1",
    "babel-loader": "^8.0.6",
    "copy-node-modules": "^1.1.1",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  }
}
